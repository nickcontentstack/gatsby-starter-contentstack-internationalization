o;\n  bottom: 20px;\n  left: 0;\n}\n\n.wrapper .container .list_section {\n  width: 100%;\n  display: none;\n  float: left;\n}\n\n.close {\n  position: absolute;\n  top: 1px;\n  right: 1px;\n  opacity: 0.3;\n  display: block;\n  box-sizing: border-box;\n  width: 20px;\n  height: 20px;\n  border-width: 3px;\n  border-style: solid;\n  border-color: #5a5a5a;\n  border-radius: 100%;\n  background: -webkit-linear-gradient(-45deg, transparent 0%, transparent 46%, white 46%, white 56%, transparent 56%, transparent 100%), -webkit-linear-gradient(45deg, transparent 0%, transparent 46%, white 46%, white 56%, transparent 56%, transparent 100%);\n  background-color: #777;\n  box-shadow: 0 0 5px 2px rgba(0, 0, 0, 0.5);\n  transition: all 0.3s ease;\n  z-index: 9;\n}\n\n.close:hover {\n  opacity: 1;\n}\n\n::-webkit-scrollbar {\n  width: 6px;\n}\n\n::-webkit-scrollbar-track {\n  border-radius: 10px;\n  background-color: transparent;\n}\n\n::-webkit-scrollbar-thumb {\n  border-radius: 6px;\n  background: #dfe2e9;\n}\n\n  </style>\n</head>\n\n<body>\n  <div class=\"wrapper\">\n    <div class=\"container\">\n      <div class=\"reference-loading\">\n        <div class=\"loading-flash\">\n          <div></div>\n          <div></div>\n          <div></div>\n          <div></div>\n        </div>\n      </div>\n      <div class=\"entry_table\" style=\"display: none;\">\n        <table>\n            <thead>\n                <tr>\n                    <th>Title</th>\n                    <th>URL</th>\n                </tr>\n            </thead>\n            <tbody>\n\n            </tbody>\n        </table>\n        <span>No Entry Selected</span>\n        <div class=\"choose_asset\" id=\"open-btn\">Choose Entries</div>\n      </div>\n    </div>\n  </div>\n\n  <script>\n    let entries = {};\n    let entryTable = $('.entry_table table tbody');\n    let loader = $('.reference-loading');\n    function render() {\n      let setData = [];\n      entryTable.empty();\n\n      /* eslint-disable no-unused-vars */\n      for (let [key, entry] of Object.entries(entries)) {\n        setData.push(entry);\n        entryTable.append(`<tr id=\"${entry.uid}\"><td>${entry.title}</td><td>${entry.url}</td><td><button id=${key}> X </button></td></tr>`);\n        $(`#${key}`).click(() => {\n          delete entries[key];\n          render();\n        });\n      }\n      if (Object.keys(entries).length > 0) {\n        $('.entry_table span').hide();\n      } else {\n        $('.entry_table span').show();\n      }\n\n      $('.close').click(function () {\n        let id = $(this).parent().parent().attr('id');\n        delete entries[id];\n        render();\n      });\n    }\n\n    function isEmpty(obj) {\n      for (let key in obj) {\n        if (Object.prototype.hasOwnProperty.call(obj, key)) return false;\n      }\n      return true;\n    }\n\n    // save data from child window (bynder widget)\n    function saveData(event) {\n      try {\n        let childData = JSON.parse(event.data);\n\n        if (childData.data !== undefined && childData.data !== null) {\n          childData.data.forEach((entry) => {\n            entries[entry.uid] = entry;\n          });\n          render();\n        }\n      } catch (e) {\n        // eslint-disable-next-line no-console\n        console.log('Error', e);\n      }\n    }\n\n    // for opening popup window\n    function openWidget() {\n      // \"url\" must be the URL for hosted widget so that on clicking of the button this URL should open in a new window\n      let url = \"https://assets.contentstack.io/v3/assets/bltbfc481de4ed665dc/blt3e1756ce1ab44e55/5fe422f3a7955d1a6a891209/index.html\";\n      window.open(url, 'popUpWindow', 'height=500,width=1200,left=100,top=100,toolbar=no,menubar=no, status=no');\n      window.addEventListener('message', saveData, false);\n    }\n\n    $(document).ready(() => {\n      loader.show();\n      $('#open-btn').click(() => {\n        openWidget();\n      });\n      setTimeout(() => {\n        $('.entry_table span').show();\n        loader.hide();\n        $('.entry_table').show();\n      }, 1000);\n      window.updateHeight(335);\n    });\n  </script>\n</body>\n\n</html>"}}